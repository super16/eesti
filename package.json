    "eslint-plugin-vue": "^9.9.0",
    "execa": "^7.1.1",
    "jsdom": "^21.1.1",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "typescript": "^5.0.2",
    "vite": "^4.2.0",
    "vitest": "^0.29.3",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.2.0"
